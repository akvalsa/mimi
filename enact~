import pysftp
host = "sftp-upload.datareadings.com"
password = "Enact@BBftp"
username = "enact_abb"

local_path = "/home/akshay/work/fa/gmail_push_notifications/my_test01.txt"
remote_path = "/upload/test"
cnopts = pysftp.CnOpts()
cnopts.hostkeys = None  
with pysftp.Connection(host, username=username, password=password, cnopts=cnopts) as sftp:
    sftp.put(local_path, remote_path)


